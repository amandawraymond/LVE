<img src="https://i.imgur.com/x2G2aLn.jpg" id="bg-list">
<div id="saved-results">
  <% provide(:title, "My Concerts") %>
  <h1 class="header1">My Concerts</h1>  <br/>

  <h1><% @my_concerts.each do |concert| %></h1>
    <%= concert.headliner %><br/>
   
     <%= concert.concert_date %><br/>
    <%= concert.performing_artists.to_s %><br/>
    @<%= concert.venue %><br/>
    in <%= concert.location %><br/>
    <%= link_to 'More Info', concert.website.html_safe, target: "_blank" %><br/>
    <%= link_to 'Delete', concert_path(concert), method: :delete, data: { confirm: 'Are you sure you want to delete this concert?' } %><br/><br/>
    ---------------------------------<br/><br/>

  <% end %>
</div>

