<html>
  <% provide(:title, "Sign In") %>

  <h1>Sign In</h1>
 <!--  <div class="jumbotron"> -->
    <%= form_for(:session, url: sessions_path) do |form| %>

      <div>
        <%= form.label :email %>
        <%= form.text_field :email %>
      </div>
      <br/>

      <div>
        <%= form.label :password %>
        <%= form.password_field :password %>
      </div>
      <br/>

      <div class="button">
        <%= form.submit 'Sign In' %>
      </div>
    <% end %>
<!--   </div> -->
</html>