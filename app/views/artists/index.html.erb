<html>
  <% provide(:title, "My Artists") %>

   <table>
    <tr>
      <th colspan="3"></th>
    </tr>
    <% @tasks.each do |task| %>
    <tr>
      <td class="title"><%= task.title %>&nbsp;&nbsp;</td>
      <td class="link"><%= link_to 'Show', task_path(task) %>&nbsp;&nbsp;</td>
      <td class="link"><%= link_to 'Delete', task_path(task), method: :delete, data: { confirm: 'Are you sure?' } %></td>    
    </tr>
    <% end %>
  </table>
  
</html>
